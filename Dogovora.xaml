<Window x:Class="Страховая.dogovora"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Страховая"
        mc:Ignorable="d"
        Title="Договора" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBlock Text="№ Договора" HorizontalAlignment="Left" Margin="10" >
                    <TextBox HorizontalAlignment="Right" Width="300" Margin="10" />
            </TextBlock>

            <TextBlock Text="Дата заключения" Margin="10" HorizontalAlignment="Left">
                <TextBox HorizontalAlignment="Right" Width="300" Margin="10" />
            </TextBlock>             
            <TextBlock Text="ФИО клиента" />
            <TextBlock Text="Паспортные данные" />
            <TextBlock Text="Объект страхования" />
            <TextBlock Text="Страховая выплата" />
            <TextBlock Text="Срок действия до" />
        </StackPanel>
    </Grid>
</Window>
