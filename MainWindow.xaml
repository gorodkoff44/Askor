        <Button Content="Страховые выплаты" Width="auto" Height="70" Margin="5" Button.Click="Button_Click_2" />
        <Button Content="Выход" Width="auto" Height="70" Margin="5" Button.Click="Button_Click_3" />
    </WrapPanel>
