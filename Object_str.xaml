<Window x:Class="Страховая.object_str"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Страховая"
        mc:Ignorable="d"
        Title="Объекты страхования" Height="450" Width="800">
    <Grid>

        <TextBlock HorizontalAlignment="Center" FontSize="40" Margin="118" Foreground="Black">Выберете объект страхования</TextBlock>
        <WrapPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <Button Content="Автомобиль" Width="150" Height="50" Margin="5" Foreground="White" Background="#55000000" BorderBrush="#33ffffff" Button.Click="Button_Click" />
            <Button Content="Жильё" Width="150" Height="auto" Margin="5" Foreground="White" Background="#55000000" BorderBrush="#33ffffff" Button.Click="Button_Click_1" />
            <Button Content="Собственная жизнь" Width="150" Height="auto" Margin="5" Foreground="White" Background="#55000000" BorderBrush="#33ffffff" Button.Click="Button_Click_2" />
        </WrapPanel>
    </Grid>
</Window>
